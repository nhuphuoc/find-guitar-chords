{
  "name": "Quickords",
  "description": "Find your song's chords quickly by one click on the youtube title!",
  "version": "1.5",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/quickords_logo.png",
    "32": "images/quickords_logo.png",
    "48": "images/quickords_logo.png",
    "128": "images/quickords_logo.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["scripts/content.js"]
    }
  ]
}
